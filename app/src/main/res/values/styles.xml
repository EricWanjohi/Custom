<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.DayNight.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/blueGrey900</item>
        <item name="colorPrimaryDark">@color/blueGrey500</item>
        <item name="colorAccent">@color/grey_900</item>
    </style>

    <style name="AppTheme.NoActionBar" parent="Theme.MaterialComponents.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/blueGrey900</item>
        <item name="colorPrimaryDark">@color/blueGrey500</item>
        <item name="colorAccent">@android:color/white</item>
    </style>

    <style name="AppTheme.UserProfile" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/blueGrey900</item>
        <item name="colorPrimaryDark">@color/blueGrey500</item>
        <item name="colorAccent">@android:color/white</item>
    </style>


</resources>
